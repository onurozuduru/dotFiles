{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#E36464",
          "foreground": "#fff",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "trailing_diamond": "\ue0c6",
          "type": "os"
        },
        {
          "background": "blue",
          "foreground": "#fff",
          "leading_diamond": "\ue0c7 ",
          "style": "diamond",
          "template": " {{ if glob \"/run/.toolboxenv\" }}\ue650 {{ replaceP \"(.*)[.](.*)\" .HostName \"${1}\" }}{{ end }} ",
          "trailing_diamond": "\ue0c6",
          "type": "text"
        },
        {
          "background": "green",
          "foreground": "#fff",
          "leading_diamond": "\ue0c7 ",
          "properties": {
            "folder_icon": "\udb80\ude55 ",
            "max_depth": 3,
            "style": "agnoster_short"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0c6",
          "type": "path"
        },
        {
          "background": "cyan",
          "foreground": "#fff",
          "leading_diamond": "\ue0c7 ",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": " \uf418 ({{ .HEAD }}) ",
          "trailing_diamond": "\ue0c6",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
